    "webpack-dev-server": "^2.4.5"
  },
  "devDependencies": {
    "autoprefixer-stylus": "^0.14.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
